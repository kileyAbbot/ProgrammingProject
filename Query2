ArrayList sortbyDestAirport(ArrayList flightsSet, String airport1){
  ArrayList <Flight> flights = flightsSet;
  ArrayList destAir = new ArrayList<Flight>();
 
  for ( int i = 0; i<flights.size(); i++ )
  {
    String airport2 = flights.get(i).destinationAirport;
    if (airport2==airport1){
      destAir.add(flights);
    }
  }
  return destAir;
  
  
}
